###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Activity
:Activity rdf:type owl:Class ;
          rdfs:comment "and effort expecting/requiring an action from a organization member or any of their resources" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Audience
:Audience rdf:type owl:Class ;
          <https://schema.org/name> "indivual or group leadership  percives will be interested in a result of an activity" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/DataQuest
:DataQuest rdf:type owl:Class ;
           rdfs:subClassOf :Project ;
           owl:disjointWith :Development ;
           rdfs:description "Qustions leadership generate request data exploration or analysis" ;
           rdfs:label "Data Question" ;
           <https://schema.org/name> "dq" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Development
:Development rdf:type owl:Class ;
             rdfs:subClassOf :Project ;
             rdfs:label "a software development task. i.e. web site update, api integration" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Implication
:Implication rdf:type owl:Class ;
             rdfs:subClassOf :Result ;
             owl:disjointWith :Outcome ;
             rdfs:comment "any implication of an outcome that is deemed note worthy by any participating in the assesment" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Optional
:Optional rdf:type owl:Class ;
          rdfs:subClassOf :Task ;
          owl:disjointWith :Required ;
          rdfs:description "an activity optional to completing a project" ;
          rdfs:label "Optional" .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Optional ;
   owl:annotatedProperty rdfs:label ;
   owl:annotatedTarget "Optional" ;
   rdfs:label "Recommened" ,
              "Stretch Goal"
 ] .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Outcome
:Outcome rdf:type owl:Class ;
         rdfs:subClassOf :Result ;
         rdfs:comment "the description/state to things effected by an activity's concequences" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Project
:Project rdf:type owl:Class ;
         rdfs:subClassOf :Activity .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Question
:Question rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Task
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty <https://schema.org/isPartOf> ;
                                                       owl:someValuesFrom :DataQuest
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:description "a task of a data quest activity" ;
          <https://schema.org/isPartOf> "ddcpm:DataQuest" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Required
:Required rdf:type owl:Class ;
          rdfs:subClassOf :Task ;
          rdfs:label "Critical" ,
                     """activity critical to completeing a project

or 

minimal requirement in activity's definition of done""" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Result
:Result rdf:type owl:Class ;
        rdfs:comment "the result of an activity at time of disengagement" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Task
:Task rdf:type owl:Class ;
      rdfs:subClassOf :Activity ;
      rdfs:description "an activity assigned to a project or team." ;
      rdfs:label "Task" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/Team
:Team rdf:type owl:Class ;
      rdfs:subClassOf :Activity .


