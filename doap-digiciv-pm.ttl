#################################################################

###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/audience
:audience rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :purpose ;
          rdfs:domain :DataQuest ;
          rdfs:range xsd:string ;
          rdfs:description "entites leadership deems a dataa quest question relevant to" ;
          rdfs:label "Audience Member" ;
          <https://schema.org/name> "dqAudMem" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/context
:context rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :purpose ;
         rdfs:domain :DataQuest ;
         rdfs:range xsd:string ;
         rdfs:description "Social justice / moral ethical context of question being asked" ;
         rdfs:label "Data Quest's Context" ;
         <https://schema.org/name> "dqConext" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/framing
:framing rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :question ;
         rdfs:domain :Task ;
         rdfs:range xsd:string .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/isStakeholder
:isStakeholder rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :audience ;
               rdfs:domain :Audience ;
               rdfs:range xsd:boolean ;
               rdfs:description "doe leadership think this Audience member will be harmed by a negative or neutral outcome of a data quest question"@en ;
               rdfs:label "Audience is percieved Stakeholder" ;
               <https://schema.org/name> "dqStakeholder" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/purpose
:purpose rdf:type owl:DatatypeProperty ;
         rdfs:domain :DataQuest ;
         rdfs:range xsd:string ;
         rdfs:description "reason leadership is asking these question" ;
         rdfs:label "DataQuest's Purpose" ;
         <https://schema.org/name> "dqPurpose" .


###  https://unit9a.github.io/doap-digiciv/sub-ont/pm/question
:question rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :DataQuest ;
          rdfs:range xsd:string ;
          rdfs:description "a question leadership request data exploration or analysis for." ;
          rdfs:label "Data Quest's Question(s)" ;
          <https://schema.org/name> "dqQuestion" .


